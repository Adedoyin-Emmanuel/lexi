{
  "name": "nodejs-template",
  "version": "1.0.0",
  "description": "A simple nodejs template for building nodejs applications",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "check-lint": "eslint [0-9]*.ts",
    "build": "rimraf ./dist && tsc index.ts --esModuleInterop true --target ES6 --module nodenext --moduleResolution nodenext --outDir dist",
    "start": "npm run build && node ./dist/index.js",
    "dev": "DEBUG=lexi && nodemon index.ts",
    "test": "jest",
    "full-test": "eslint */[0-9]*.ts && jest --watchAll --verbose",
    "full-test-fix": "eslint */[0-9]*.ts --fix && jest --watchAll --verbose",
    "studio": "bunx prisma studio",
    "migrate": "bunx prisma migrate dev",
    "generate": "bunx prisma generate"
  },
  "proxy": true,
  "keywords": [],
  "author": "Adedoyin Emmanuel Adeniyi",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/node": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-slow-down": "^2.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.1",
    "eslint": "^9.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.13.5",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@typegoose/typegoose": "^12.18.0",
    "@types/express-session": "^1.18.2",
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "cheerio": "^1.1.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.2",
    "got-scraping": "^4.1.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.2",
    "morgan": "^1.10.0",
    "openai": "^5.13.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "redis": "^5.8.2",
    "ts-node": "^10.9.2",
    "tsfluent": "^1.5.2"
  }
}
